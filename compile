clang++ -shared flatten.cpp  -fPIC -o pass.so
clang++ -fpass-plugin=./pass.so -S -emit-llvm -o main.ll nested_for_loop.cpp -fPIC
opt -passes=dot-cfg --disable-output main.ll
dot -Tpng .main.dot -o ./cfg/forLoopNested_obfuscated.png

clang++ -S -emit-llvm -o main.ll nested_for_loop.cpp -fPIC
opt -passes=dot-cfg --disable-output main.ll
dot -Tpng .main.dot -o ./cfg/forLoopNested_normal.png

clang++ -fpass-plugin=./pass.so -o fLoopNested_o nested_for_loop.cpp -fPIC
clang++ -o fLoopNested nested_for_loop.cpp

